{
  "semi": true,
  "tabWidth": 2,
  "printWidth": 100,
  "trailingComma": "es5",
  "singleQuote": true,
  "jsxSingleQuote": false,
  "bracketSpacing": true,
  "bracketSameLine": false,
  "arrowParens": "avoid", 
  "endOfLine": "lf",
  "quoteProps": "as-needed",
  "useTabs": true,
  "vueIndentScriptAndStyle": true,
  "htmlWhitespaceSensitivity": "css",
  "plugins": ["prettier-plugin-tailwindcss"],
  "overrides": [
    {
      "files": "*.vue","options": {
        "parser":  "vue"
      }
    },

    {
      "files": "*.ts",
      "options": {
        "parser": "typescript"    
      }
    }
  
  ]
}